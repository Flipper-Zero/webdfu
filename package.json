{
  "name": "dfu",
  "version": "0.1.1",
  "description": "Driver for working with DFU in a browser over Web USB",
  "main": "dist/index.js",
  "files": ["dist", "package.json", "package-lock.json", "readme.md"],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "demo": "parcel -d demo/dist demo/index.html",
    "format": "prettier --write .",
    "test:types": "tsc --build tsconfig.json",
    "test": "npm run test:types",
    "clear": "rm -rf .cache dist tsconfig.tsbuildinfo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flipper-Zero/webdfu.git"
  },
  "keywords": [
    "dfu",
    "web",
    "flipper"
  ],
  "author": "Nikolay Govorov <me@govorov.online> (https://govorov.online)",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/Flipper-Zero/webdfu/issues"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "homepage": "https://github.com/Flipper-Zero/webdfu#readme",
  "devDependencies": {
    "@types/w3c-web-serial": "^1.0.1",
    "@types/w3c-web-usb": "^1.0.4",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.0",
    "rollup": "^2.52.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "nanoevents": "^6.0.0"
  }
